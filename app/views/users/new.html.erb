<div class='container'>
  <h1>Signup!</h1>

  <%= form_for :user, url: '/users' do |f| %>
    <p>Name: <%= f.text_field :name %></p>
    <p>Email: <%= f.text_field :email %></p>
    <p>Username: <%= f.text_field :username %></p>
    <p>Password: <%= f.password_field :password %></p>
    <p>Password Confirmation: <%= f.password_field :password_confirmation %></p>
    <p><%= f.submit "Submit" %></p>

  <% end %>
</div>
